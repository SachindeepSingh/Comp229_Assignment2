

@model IEnumerable<Comp229_Assignment2.Models.Non_Vegfood>
@{
    ViewBag.Title = "Non_vegfood";
}
<h2>Appetizers</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Non_VegfoodName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Non_VegfoodShortDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Non_VegfoodPrice)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Non_VegfoodName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Non_VegfoodShortDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Non_VegfoodPrice)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.Non_VegfoodId }, new { @class = "btn btn-success" })
            </td>
        </tr>
    }
</table>
